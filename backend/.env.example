# Example environment variables
# Copy this file to .env and fill in your values

# Optional: API-Football key (free tier: 100 requests/day)
# Get your key at: https://api-football.com
API_FOOTBALL_KEY=

# Optional: Football-Data.org key (free tier: 10 requests/min)
# Get your key at: https://football-data.org/client/register
FOOTBALL_DATA_ORG_KEY=

# Optional: TheSportsDB key (free tier: uses '3')
THESPORTSDB_KEY=3

# Frontend URL for CORS (comma-separated for multiple)
CORS_ORIGINS=http://localhost:3000,http://localhost:5173

# Server port
PORT=8000
